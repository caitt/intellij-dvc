<idea-plugin>
    <id>com.davidprihoda.dvc</id>
    <name>dvc-support-poc</name>
    <description>DVC file support proof of concept.</description>
    <vendor email="david.prihoda@gmail.com" url="http://davidprihoda.com">David Příhoda</vendor>

    <idea-version since-build="171" />
    <depends>com.intellij.modules.lang</depends>
    <depends>org.jetbrains.plugins.yaml</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileTypeFactory implementation="com.davidprihoda.dvc.DvcFileTypeFactory"/>
        <psi.referenceContributor language="yaml"
                                  implementation="com.davidprihoda.dvc.DvcReferenceContributor"/>
    </extensions>


</idea-plugin>
